<?xml version="1.0" ?>
<robot name="Tesla_2DOF_Arm">
   <link name="base_link">
      
    
      <inertial>
         
        
         <mass value="10.0"/>
         
        
         <origin xyz="0.00.00.0"/>
         
        
         <inertia ixx="1.0" iyy="1.0" izz="1.0"/>
         
    
      </inertial>
      

   </link>
   <link name="link_1">
      
    
      <inertial>
         
        
         <mass value="5.0"/>
         
        
         <origin xyz="0.50.00.0"/>
         
        
         <inertia ixx="0.5" iyy="0.5" izz="0.5"/>
         
    
      </inertial>
      

   </link>
   <link name="link_2">
      
    
      <inertial>
         
        
         <mass value="3.0"/>
         
        
         <origin xyz="0.40.00.0"/>
         
        
         <inertia ixx="0.3" iyy="0.3" izz="0.3"/>
         
    
      </inertial>
      

   </link>
   <joint name="shoulder_joint" type="revolute">
      
    
      <parent link="base_link"/>
      
    
      <child link="link_1"/>
      
    
      <axis xyz="001"/>
      
    
      <limit effort="100.0" velocity="10.0" lower="(-2.5, 2.5)" upper="(-2.5, 2.5)"/>
      

   </joint>
   <joint name="elbow_joint" type="revolute">
      
    
      <parent link="link_1"/>
      
    
      <child link="link_2"/>
      
    
      <axis xyz="001"/>
      
    
      <limit effort="100.0" velocity="10.0" lower="(-1.5, 1.5)" upper="(-1.5, 1.5)"/>
      

   </joint>
</robot>
